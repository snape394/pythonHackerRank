from collections import Counter
n=int(input())
arr=list(map(int,input().split()))
print(sum(arr)/n)
arr=sorted(arr)
print((arr[n//2-1]+arr[n//2])/2)
c=Counter(arr)
mx=0
ke=0
for k,v in c.items():
    if(v>mx):
        mx=v
        ke=k
print(ke)

